#ifndef SOF_CONFIG_INCLUDED
#define SOF_CONFIG_INCLUDED

// Use BMI2 instruction set?
#cmakedefine USE_BMI2

// The system has stpcpy function?
#cmakedefine USE_SYSTEM_STPCPY

// Print stacktraces using boost::stacktrace?
#cmakedefine USE_BOOST_STACKTRACE

// Build without exceptions?
#cmakedefine USE_NO_EXCEPTIONS

// Build with diagnostics
#cmakedefine USE_SEARCH_DIAGNOSTICS

#endif  // SOF_CONFIG_INCLUDED
